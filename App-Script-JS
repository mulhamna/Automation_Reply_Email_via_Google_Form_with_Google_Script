Automated Email Reminder in Google Sheet

function emailReminder(){

    let spreadsheet = SpreadsheetApp.openById(masukin kode link),
        sheet = spreadsheet.getActiveSheet(),
        LastRow = sheet.getLastRow(),
        LastCol = sheet.getLastColumn(),
        data = sheet.getRange(row, column, numRows, numColumns);

    let myEmail = 'mulham@sertifikasiku.com' ;

    for (let index = 0; index < lastRow-1; index++){

        let end = data[index][1];
        end = Utilities.formatDate(end, 'GMT+8', 'MM/dd');
        let today = new Date();
        today = Utilities.formatDate(birthday, 'GMT+8', 'MM/dd');

        if (end == today){

            MailApp.sendEmail(myEmail, "Today is " + data[index][0]);

        }

    }
}
